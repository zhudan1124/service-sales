    "remove:dist": "rimraf dist",
    "remove:build": "rimraf build",
    "lint": "esw webpack.*.config.js src",
    "lint:watch": "npm run lint -- --watch",
    "rmnm": "rimraf node_modules",
    "test": "jest"
  },
