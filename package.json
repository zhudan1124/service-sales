    "dev": "set NODE_ENV=dev&& ./node_modules/.bin/webpack-dev-server --config ./webpack/webpack.dev.config.js --hot && npm run lint:watch",
    "pp": "npm run remove:dist && ./node_modules/.bin/webpack --config ./webpack/webpack.pp.config.js --progress --colors --bail",
